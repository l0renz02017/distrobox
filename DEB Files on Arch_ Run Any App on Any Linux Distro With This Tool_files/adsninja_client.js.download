        };\n\n        this.__firstAdsLoaderQueueProcessing = false;\n    }\n};\n\n//\n// Handles adsManager callback.\nValstreamVideoPlayer.prototype.initializeAds = function(){\n\n    if(!this.isAllowedToPlayAds() || this.adDisplayInitialized || !this.player.hasOwnProperty('ima') || typeof this.player.ima.initializeAdDisplayContainer !== 'function'){\n\n        return;\n    }\n\n    this.adDisplayInitialized = true;\n\n    this.player.ima.initializeAdDisplayContainer();\n}\n\n//\n// Handles adsManager callback.\nValstreamVideoPlayer.prototype.__createDefaultAdSchedule = function(){\n    \n    return {\n        preRolls: {\n            amount: this.DEFAULT_MAX_AD_PREROLL_COUNT,\n        },\n        midRolls: {\n            amount: 1,\n            intervalMin: 45,\n            intervalMax: 300,  \n            intervalMultiplier: 2,\n        },\n        postRolls: {\n            amount: 0,\n        },\n    };\n}\n\n//\n// Sets ad schedule.\nValstreamVideoPlayer.prototype.setAdSchedule = function(adSchedule){\n    \n    this.__adSchedule                = (this.isAllowedToPlayAds() && adSchedule && typeof adSchedule === 'object') ? adSchedule || this.__createDefaultAdSchedule() : null; \n    this.maxPrerollAdCount           = (this.__adSchedule && this.__adSchedule.hasOwnProperty('preRolls') && this.__adSchedule.preRolls.hasOwnProperty('amount')) ? this.__adSchedule.preRolls.amount : this.DEFAULT_MAX_AD_PREROLL_COUNT;\n    this.midrollThresholdRequest     = 5;\n    this.midrollThresholdAuction     = (this.__adSchedule && this.__adSchedule.hasOwnProperty('midRolls') && this.__adSchedule.midRolls.hasOwnProperty('intervalMin')) ? this.__adSchedule.midRolls.intervalMin - this.midrollThresholdRequest : 40;\n    this.midrollIntervalMax          = (this.__adSchedule && this.__adSchedule.hasOwnProperty('midRolls') && this.__adSchedule.midRolls.hasOwnProperty('intervalMax')) ? this.__adSchedule.midRolls.intervalMax - this.midrollThresholdRequest : 300 - this.midrollThresholdRequest;\n    this.midrollIntervalMultiplier   = (this.__adSchedule && this.__adSchedule.hasOwnProperty('midRolls') && this.__adSchedule.midRolls.hasOwnProperty('intervalMultiplier')) ? this.__adSchedule.midRolls.intervalMultiplier : 2;\n    this.baseMidrollThresholdRequest = this.midrollThresholdRequest;\n    this.baseMidrollThresholdAuction = this.midrollThresholdAuction;\n    this.nextMidrollThresholdRequest = this.baseMidrollThresholdAuction + this.baseMidrollThresholdRequest;\n    this.nextMidrollThresholdAuction = this.baseMidrollThresholdAuction;\n}\n\n//\n// callback for preparing for next playback.\nValstreamVideoPlayer.prototype.prepareForNextPlaybackCallback = function(){\n\n    this.cancelAutoNav(); // Precaution.\n\n    if(!this.isEnabledAds() || (this.isEnabledAds() && !this.hasPlaylist && !this.isAutoplayEnabled())){\n\n        this.hasPreparedForNextPlayback = false;\n\n        return;\n    }\n\n    this.hasPreparedForNextPlayback = true;\n\n    // TODO: Maybe check if you can play ads here with by checking existence of \"player.ima\" and \"player.ads\".\n\n    // Reset properties.\n    this.adErrorRetriesExhausted     = false;\n    this.prerollCount                = 0;\n    this.adErrorRetries              = 0; // Reset retries.\n    this.currentPlacement            = 'preroll';\n    this.midRollUpcoming             = false;\n    this.resumingTimeStamp           = null;\n    this.adFinishing                 = false;\n    this.videoAdQueue                = [];\n    this.baseMidrollThresholdAuction = this.midrollThresholdAuction;\n    this.baseMidrollThresholdRequest = this.midrollThresholdRequest;\n    this.nextMidrollThresholdAuction = this.baseMidrollThresholdAuction;\n    this.nextMidrollThresholdRequest = this.baseMidrollThresholdAuction + this.baseMidrollThresholdRequest;\n    this.numberOfMidrolls            = 0;\n\n    // TODO: Not needed?\n    if(this.allowAutoplay){\n\n        this.initializeAds();\n    }\n\n    var self = this;\n\n    // Need to wait for ads loader to be available.\n    this.__adsLoaderQueue.push(function(){\n\n        // Start next auction.\n        self.requestNextAuction('preroll');\n    });\n\n    if(!this.allowAutoPlaylistNavigation){\n\n        this.logger.log('Next mid-roll scheduled.', {\n            elementId: this.elementId,\n            currentTime: 0,\n            numberOfMidrollsToDate: this.numberOfMidrolls,\n            baseMidrollThresholdAuction: this.baseMidrollThresholdAuction,\n            baseMidrollThresholdRequest: this.baseMidrollThresholdRequest,\n            nextMidrollThresholdAuction: this.nextMidrollThresholdAuction,\n            nextMidrollThresholdRequest: this.nextMidrollThresholdRequest,\n        });\n    }\n};\n\n//\n// Prepares for next playback.\nValstreamVideoPlayer.prototype.prepareForNextPlayback = function(nextPlayBackDelay = 0){\n\n    // If no ad is play or is in the queue, request next.\n    if(!this.hasPlaylist && (this.hasPreparedForNextPlayback || !this.isAutoplayEnabled() || this.videoAdQueue.length !== 0 || this.adPlaying)){\n        \n        return;\n    }\n    \n    if(nextPlayBackDelay){\n\n        setTimeout(this.prepareForNextPlaybackCallback.bind(this), nextPlayBackDelay);\n    }\n    else{\n\n        this.prepareForNextPlaybackCallback();\n    }\n}\n\n//\n// Handles ad mute button.\nValstreamVideoPlayer.prototype.handleVolumeToggle = function(event = null, context = null){\n\n    if(context == 'mute' || (!context && this.largeSoundToggleButtonElement.classList.contains('an-vp-unmuted'))){\n\n        // Mute.\n        this.player.muted(true);\n\n        // Set session cookie for instream only\n        if('instream' === this.videoType){\n\n            document.cookie = \"muteStatus=muted; path=/\";\n        }\n\n        if(this.googleImaAdManager){\n\n            try{\n\n                this.googleImaAdManager.setVolume(0);\n            }\n            catch(error){\n\n                // Do nothing.\n            }\n        }\n\n        if(this.adVolumeSliderLevelElement){\n\n            this.adVolumeSliderLevelElement.style.width = '0px';\n        }\n\n        if(this.largeSoundToggleButtonElement){\n\n            this.largeSoundToggleButtonElement.classList.remove('an-vp-unmuted');\n            this.largeSoundToggleButtonElement.classList.add('an-vp-muted');\n        }\n\n        if(this.adMuteElement){\n            \n            this.adMuteElement.classList.remove('ima-non-muted');\n            this.adMuteElement.classList.add('ima-muted');\n        }\n    }\n    else if(context == 'unmute' || (!context && this.largeSoundToggleButtonElement.classList.contains('an-vp-muted'))){\n\n        // Un-mute.\n        this.player.muted(false);\n\n        // Set session cookie for instream only\n        if('instream' === this.videoType){\n\n            document.cookie = \"muteStatus=unmuted; path=/\";\n        }\n\n        var volume = this.player.volume();\n\n        if(this.googleImaAdManager){\n\n            try{\n\n                this.googleImaAdManager.setVolume(volume);\n            }\n            catch(error){\n\n                // Do nothing.\n            }\n        }\n        \n        if(this.adVolumeSliderLevelElement){\n\n            this.adVolumeSliderLevelElement.style.width = `${volume * 100}%`;\n        }\n\n        if(this.largeSoundToggleButtonElement){\n            \n            this.largeSoundToggleButtonElement.classList.remove('an-vp-muted');\n            this.largeSoundToggleButtonElement.classList.add('an-vp-unmuted');\n        }\n\n        if(this.adMuteElement){\n\n            this.adMuteElement.classList.remove('ima-muted');\n            this.adMuteElement.classList.add('ima-non-muted');\n        }\n    }\n}\n\n//\n// Register event listeners.\nValstreamVideoPlayer.prototype.__registerEventListeners = function(){\n    \n    var self       = this;\n    var handleMute = function(event){\n        \n        event.preventDefault(); \n        self.handleVolumeToggle(event);\n    }\n\n    // Mute button.\n    this.largeSoundToggleButtonElement.addEventListener('click', this.handleVolumeToggle.bind(this));\n    this.muteButtonElement.addEventListener('click', this.handleVolumeToggle.bind(this));\n    this.largeSoundToggleButtonElement.addEventListener('touchend', handleMute);\n    this.muteButtonElement.addEventListener('touchend', handleMute);\n}\n\n//\n// Register ad event listeners.\nValstreamVideoPlayer.prototype.__registerAdEventListeners = function(){\n    \n    var self       = this;\n    var handleMute = function(event){\n        \n        event.preventDefault(); \n        self.handleVolumeToggle(event);\n    }\n\n    // Mute button.\n    this.adMuteElement.addEventListener('click', this.handleVolumeToggle.bind(this));\n    this.adMuteElement.addEventListener('touchend', handleMute);\n}\n\n//\n// Handle overlay title element hide and show.\nValstreamVideoPlayer.prototype.handleOverlayTitleElement = function(timeoutResetOnly = false, extraTime = 0) {\n  \n    var self = this;\n            \n    // Clear any existing timeouts\n    if (self.titleTimeout) {\n    \n        clearTimeout(self.titleTimeout);\n    }\n\n    // early return to hide title and cancel any timeouts\n    if(timeoutResetOnly){\n\n        return;\n    }\n\n    // skip trigger from mouse events during ad playing and before video has started\n    if(!this.adPlaying && this.player.hasStarted_) {\n        \n        // only for instream\n        if('instream' === this.videoType){\n            \n            self.overlayTitleElement.classList.remove('an-vp-int-title-slide-up');\n            \n            // Hide the title after 2 seconds + extraTime\n            self.titleTimeout = setTimeout(function() {\n                \n                self.overlayTitleElement.classList.add('an-vp-int-title-slide-up');\n            }, 2000 + extraTime);\n        }\n    }\n}\n\n//\n// Toggle autoplay.\nValstreamVideoPlayer.prototype.__toggleAutoplay = function(value){\n\n    this.__autoplay = value;\n}\n\n//\n// Enable autoplay.\nValstreamVideoPlayer.prototype.enableAutoplay = function(){\n    \n    this.__toggleAutoplay(true);\n}\n\n//\n// Disable autoplay.\nValstreamVideoPlayer.prototype.disableAutoplay = function(){\n    \n    this.__toggleAutoplay(false);\n}\n\n//\n// Check if autoplay is enabled.\nValstreamVideoPlayer.prototype.isAutoplayEnabled = function(){\n    \n    return (this.__autoplay && this.allowAutoplay);\n}\n\n//\n// Initialization.\nValstreamVideoPlayer.prototype.__init = function(){\n\n    if(this.initialized){\n\n        return;\n    }\n\n    this.initialized = true;\n\n    //this.recordEvent('Load');\n    this.recordSentinelEvent('Load');\n\n    this.adElement = document.getElementById(this.elementId);\n\n    if(!this.adElement){\n\n        this.logger.error(`Unable to setup video player. Cannot find element with ID: ${this.elementId}.`);\n        return;\n    }\n\n    // Check if there's already something initialized in the div.\n    if(this.adElement.getElementsByTagName('video').length > 0){\n\n        this.logger.error(`Unable to setup video player. Non-empty container element with ID: ${this.elementId}.`);\n        return;\n    }\n\n    // Inject video player into target container.\n    this.adElement.innerHTML = this.buildPlayerDom();\n\n    this.videoPlayerElement = this.adElement.querySelector('.an-vp-int');\n\n    if(!this.videoPlayerElement){\n        \n        this.logger.error(`Unable to setup video player. Cannot find video player element.`);\n        return;\n    }\n\n    this.videoContainer = this.adElement.querySelector('.an-vp-int-c-video');\n\n    if(!this.videoContainer){\n\n        this.logger.error(`Unable to setup video player. Cannot find video container.`);\n        return;\n    }\n\n    this.videoDetailsElement = this.adElement.querySelector('.an-vp-int-details');\n\n    if(!this.videoDetailsElement){\n\n        this.logger.error(`Unable to setup video player. Cannot find video details element.`);\n        return;\n    }\n\n    this.overlayElement = this.adElement.querySelector('.an-vp-int-overlays');\n\n    if(!this.overlayElement){\n\n        this.logger.error(`Unable to setup video player. Cannot find overlay title element.`);\n        return;\n    }\n\n    this.overlayTitleElement = this.overlayElement.querySelector('.an-vp-int-title');\n\n    if(!this.overlayTitleElement){\n\n        this.logger.error(`Unable to setup video player. Cannot find overlay title element.`);\n        return;\n    }\n\n    this.largeSoundToggleButtonElement = this.adElement.querySelector('.an-vp-btn-mute');\n\n    if(!this.largeSoundToggleButtonElement){\n        \n        this.logger.error(`Unable to setup video player. Cannot find large mute button.`);\n        return;\n    }\n\n    // this.overlayDurationElement = this.adElement.querySelector('.an-vp-int-duration-overlay');\n    //\n    // if(!this.overlayDurationElement){\n    //\n    //     this.logger.error(`Unable to setup video player. Cannot find overlay duration element.`);\n    //     return;\n    // }\n\n    this.videoContainerInner = this.videoContainer.querySelector('.an-vp-int-c-video-inner');\n    this.autoNav             = this.videoContainer.querySelector('.an-vp-int-auto-nav-container');\n    this.autoNavNext         = this.videoContainer.querySelector('.an-vp-int-auto-nav.an-vp-int-next');\n    this.autoNavStay         = this.videoContainer.querySelector('.an-vp-int-auto-nav.an-vp-int-stay');\n    this.adSkipCountdown     = this.videoContainer.querySelector(`.an-vp-int-skip-counter`);\n    this.adSkipNext          = this.videoContainer.querySelector(`.an-vp-int-skip-text`);\n    this.adSkipButton        = this.videoContainer.querySelector(`.an-vp-int-skip-btn`);\n    this.titleElement        = this.videoDetailsElement.querySelector(`.an-vp-int-title`);\n    // this.descriptionElement  = this.videoDetailsElement.querySelector(`.an-vp-int-video-description`);\n    this.video               = this.videoContainer.querySelector('video');\n\n    if(!this.videoContainerInner || !this.autoNav || !this.autoNavNext || !this.video){\n\n        this.logger.error(`Unable to setup video player. Cannot find one or more video container elements.`, {\n            videoContainerInner: this.videoContainerInner,\n            autoNav: this.autoNav,\n            autoNavNext: this.autoNavNext,\n            video: this.video,\n        });\n\n        return;\n    }\n\n    var self = this;\n\n    window['$vvv'] = {};\n\n    // Init videojs.\n    this.player = this.videoJsLibrary(this.video, {\n        muted: (!this.soundOn),\n        autoplay: false,\n        fluid: (this.MODE_LAYOUT_FLUID === this.modeLayout),\n        fill: (this.MODE_LAYOUT_FILL === this.modeLayout),\n        responsive: true,\n        controls: true,\n        aspectRatio: '16:9'\n    }, function(){\n\n        var player         = this;\n        var playlistVideos = [];\n\n        if(self.playlistVideoUrls && self.playlistVideoUrls.length > 0){\n\n            self.hasPlaylist = true;\n\n            // Set video playlist.\n            playlistVideos = self.playlistVideoUrls.map(function(url){\n\n                return {\n                    controls: true,\n                    sources: [\n                        {\n                            src: url,\n                            type: 'video/mp4',\n                        },\n                    ],\n                }\n            });\n        }\n        else if(self.playlist && self.playlist.length > 0){\n\n            self.hasPlaylist = true;\n\n            for(var p in self.playlist){\n\n                var playlistItem = {\n                    controls: true,\n                    sources: [\n                        {\n                            src: self.playlist[p]['url'],\n                            type: self.playlist[p]['mimeType'] || 'video/mp4',\n                        },\n                    ],\n                    title: self.playlist[p]['title'] || '',\n                    description: self.playlist[p]['description'] || '',\n                    duration: self.playlist[p]['duration'] || '',\n                };\n\n                // Add posters when needed.\n                if(self.playlist[p].hasOwnProperty('thumbnailLink') && self.playlist[p]['thumbnailLink']){\n\n                    playlistItem['poster'] = self.playlist[p]['thumbnailLink'];\n                }\n\n                playlistVideos.push(playlistItem);\n            }\n        }\n\n        if(self.isAllowedToPlayAds()){\n\n            // Important to initialize IMA soon after videoJS.\n            player.ima({\n                debug: true,\n                prerollTimeout: self.prerollTimeoutInMilliseconds + 500,\n                vastLoadTimeout: 10000,\n                numRedirects: 10,\n                disableCustomPlaybackForIOS10Plus: true,\n                forceNonLinearFullSlot: true,\n                autoPlayAdBreaks: (self.hasPlaylist),\n            });\n\n            if(self.allowAutoplay){\n\n                // *** DO NOT REMOVE *** Needed by Optimera for their targeting.\n                window['$vvv']['ima'] = player.ima;\n            }\n\n            // Ads rendering settings.\n            // Event will fire constantly. Event listeners need to be re-defined each time.\n            player.on('ads-manager', self.adsManagerLoadedCallback.bind(self));\n            player.on('ads-loader', self.adsLoaderLoadedCallback.bind(self));\n        }\n        else{\n\n            self.disableAds();\n\n            self.logger.warn('Unable to initialize IMA.', {\n                player: player,\n            }, false);\n        }\n\n        if(!self.allowAutoplay){\n\n            // Instream initialization to block ad play until user clicks.\n            var startEvent = 'click';\n\n            if(navigator.userAgent.match(/iPhone/i)\n                || navigator.userAgent.match(/iPad/i)\n                || navigator.userAgent.match(/Android/i)\n            ){\n\n                startEvent = 'touchend';\n            }\n\n            if(self.adElement){\n\n                self.adElement.addEventListener(startEvent, self.initializeAds.bind(self), { once: true });\n            }\n        }\n        else{\n\n            // *** DO NOT REMOVE *** Needed by Optimera for their targeting.\n            window['$vvv']['videoJS'] = player;\n        }\n\n        // Add additional controls to videoJS.\n        var volumePanel           = player.controlBar.getChild('VolumePanel');\n        var fullscreenToggle      = player.controlBar.getChild('FullscreenToggle');\n        var fullscreenToggleIndex = player.controlBar.children().indexOf(fullscreenToggle);\n        var volumePanelIndex      = player.controlBar.children().indexOf(volumePanel);\n        var nextButton            = player.controlBar.addChild('button', {}, volumePanelIndex);\n        var prevButton            = player.controlBar.addChild('button', {}, volumePanelIndex);\n        var skipForwardButton     = player.controlBar.addChild('button', {}, fullscreenToggleIndex);\n        var skipBackButton        = player.controlBar.addChild('button', {}, fullscreenToggleIndex);\n\n        prevButton.addClass('vjs-prev-button-control');\n        prevButton.addClass('vjs-nav-button-control');\n        nextButton.addClass('vjs-next-button-control');\n        nextButton.addClass('vjs-nav-button-control');\n        skipBackButton.addClass('vjs-skip-backward-control');\n        skipBackButton.addClass('vjs-skip-button-control');\n        skipForwardButton.addClass('vjs-skip-forward-control');\n        skipForwardButton.addClass('vjs-skip-button-control');\n\n        // Attach event listeners.\n        self.videoContainer.addEventListener('click', function(event){\n\n            self.handleClickEvent(event);\n        });\n\n        self.videoContainer.addEventListener('touchend', function(event){\n\n            // To avoid duplicate event fires.\n            // event.preventDefault();\n\n            self.handleClickEvent(event);\n        });\n\n        self.videoContainer.addEventListener('mouseenter', function(event){\n        \n            self.handleOverlayTitleElement();\n        });\n\n        // Stop the current ad playing and return to content.\n        self.adSkipButton.addEventListener('click', function(event){\n\n            self.handleAdSkip(self.delayAfterAdSkippedInMilliseconds);\n        });\n\n        // Stop the current ad playing and return to content.\n        self.adSkipButton.addEventListener('touchend', function(event){\n\n            // To avoid duplicate event fires.\n            event.preventDefault();\n\n            self.handleAdSkip(self.delayAfterAdSkippedInMilliseconds);\n        });\n\n        // Track time on page.\n        window.setInterval(function(){\n            // TODO: Maybe refactor with waypoints.\n\n            if(self.intersectedViewport){\n\n                self.duration += 1;\n\n                if(self.isVisible){\n\n                    self.viewableDuration += 1;\n                }\n            }\n        }, 1000);\n\n        window.addEventListener('beforeunload', function(){\n\n            // self.recordEvent('duration', '', {\n            //     duration: self.duration,\n            // });\n            // self.recordEvent('viewableDuration', '', {\n            //     viewableDuration: self.viewableDuration,\n            // });\n        });\n\n        // When user leaves the tab.\n        // Value of isVisible is always true unless the player is closed.\n        document.addEventListener('visibilitychange', function(){\n\n            if(!self.isAutoplayEnabled() || !self.allowAutoplayWhenInView){\n\n                return;\n            }\n\n            if(self.isVisible && document.visibilityState === 'visible' && self.adPlaying && self.isEnabledAds()){\n\n                self.player.ima.resumeAd();\n            }\n            else if(self.isVisible && document.visibilityState === 'visible'){\n\n                self.player.play();\n                self.startAutoNav();\n            }\n            else if(self.isVisible){\n\n                self.player.pause();\n                self.cancelAutoNav();\n\n                if(self.isEnabledAds() && self.player.ads.inAdBreak()){\n\n                    self.player.ima.pauseAd();\n                }\n            }\n        });\n\n        player.on('adsready', function(event){\n\n            self.logger.log('Ads ready.', {\n                event: event,\n            }, false);\n\n            if(!self.hasPlaylist){\n\n                self.player.ima.playAdBreak();\n            }\n        });\n\n        player.on('readyforpreroll', function(event){\n\n            self.logger.log('Ready for Pre-roll.', {\n                event: event,\n            }, false);\n        });\n\n        player.on('adtimeout', function(event){\n\n            self.logger.log('Ad timeout.', {\n                event: event,\n            }, false);\n\n            self.handleAdDone();\n        });\n\n        player.on('error', function(event){\n\n            self.cancelAutoNav();\n\n            self.logger.log('Error.', {\n                event: event,\n            }, false);\n\n            // Sample 5% of errors\n            var sampleErrors = Math.floor(Math.random() * (100 - 1 + 1) + 1);\n\n            if(sampleErrors < 6){\n\n                var message      = 'Unknown error.';\n                var eventMessage = event.message;\n\n                // self.recordEvent('adError', '', {\n                //     message: eventMessage || message,\n                // });\n            }\n\n            // Skip to next video on video playback errors.\n            self.playlistNext();\n        });\n\n        //\n        // If some ad error occurs while trying to play an ad, retry up to 3 times to play a different ad.\n        player.on('adserror', function(event){\n\n            self.cancelAutoNav();\n\n            self.logger.log('Ads error.', {\n                event: event,\n            }, false);\n\n            if(typeof event.getAd === 'function'){\n\n                self.logger.log('Ads error.', {\n                    ad: event.getAd(),\n                }, false);\n            }\n\n            // Sample 5% of errors\n            var sampleErrors = Math.floor(Math.random() * (100 - 1 + 1) + 1);\n\n            if(sampleErrors < 6){\n\n                var errorMessage = 'Unable to determine error message.';\n\n                if(event.hasOwnProperty('data') && event.data.hasOwnProperty('AdError') && typeof event.data.AdError.getInnerError === 'function'){\n\n                    errorMessage = event.data.AdError.getInnerError();\n                }\n\n                // self.recordEvent('adError', '', {\n                //     message: errorMessage,\n                // });\n            }\n\n            self.logger.log(`Ads error, attempting to retry. Retry count: ${self.adErrorRetries}.`, {\n                retryCount: self.adErrorRetries,\n                event: event,\n            }, false);\n\n            var bidsAvailable = true;\n\n            if(event.hasOwnProperty('data') && event.data.hasOwnProperty('AdError') && typeof event.data.AdError.getErrorCode === 'function'){\n\n                bidsAvailable = (event.data.AdError.getErrorCode() !== 1009);\n            }\n\n            // Don't keep retrying when no VAST response returned. Cuts down on GAM requests.\n            if(self.hasPlaylist && self.adErrorRetries < self.maxAdErrorRetries && bidsAvailable){\n\n                // Retry ad rendering.\n                var videoUrl = self.biddingManager.retryPrerollAd(self.elementId);\n\n                self.logger.log('Retrying pre-roll ad.', {\n                    elementId: self.elementId,\n                    adErrorRetries: self.adErrorRetries,\n                    videoUrl: videoUrl,\n                });\n\n                self.adErrorRetries += 1;\n\n                if(videoUrl){\n\n                    self.__renderAd(videoUrl, sel