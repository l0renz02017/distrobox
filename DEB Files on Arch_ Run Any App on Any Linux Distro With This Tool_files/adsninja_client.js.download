its);\n\n                window.pbjs.requestBids({\n                    adUnits: prebidAdUnits,\n                    bidsBackHandler: function(bids, timedOut, prebidAuctionId){\n\n                        window.googletag.cmd.push(function(){\n\n                            if(!window.adsNinjaHeaderBiddingManager.areBidPoolsEnabled()){\n\n                                var displayAdUnitCodes = window.adsNinjaHeaderBiddingManager.auctions[auctionId].getPrebidAdUnitCodesNonVideo();\n\n                                if(displayAdUnitCodes && displayAdUnitCodes.length > 0){\n\n                                    window.pbjs.setTargetingForGPTAsync(displayAdUnitCodes);\n                                }\n                            }\n                            else if(window.adsNinjaHeaderBiddingManager.areBidPoolsEnabled() && context !== 'video'){\n\n                                window.adsNinjaHeaderBiddingManager.addNewPrebidBidsToPools(bids);\n                            }\n\n                            window.adsNinjaHeaderBiddingManager.log(`Prebid bids for auction: ${auctionId}.`, {\n                                bids: bids,\n                                timedOut: timedOut,\n                                prebidAuctionId: prebidAuctionId,\n                            });\n\n                            // var videoAdUnit = window.adsNinjaHeaderBiddingManager.auctions[auctionId].getVideoPrebidAdUnit();\n                            //\n                            // Check if we have an instream video player ad.\n                            // if(videoAdUnit){\n                            //\n                            //     window.adsNinjaHeaderBiddingManager.log('Managed video error, somehow using display focused Prebid!');\n                            //\n                            //     // if(false){\n                            //     //\n                            //     //\n                            //     //     var videoUrl = window.pbjs.adSer